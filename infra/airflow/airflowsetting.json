{
  "connections": [
    {
      "conn_id": "warehouse",
      "conn_type": "postgres",
      "host": "postgres",
      "schema": "${PG_DB}",
      "login": "${PG_USER}",
      "password": "${PG_PASSWORD}",
      "port": 5432,
      "extra": null
    },
    {
      "conn_id": "minio_s3",
      "conn_type": "aws",
      "login": "${MINIO_ROOT_USER}",
      "password": "${MINIO_ROOT_PASSWORD}",
      "host": "minio:9000",
      "extra": {
        "endpoint_url": "http://minio:9000",
        "region_name": "us-east-1",
        "verify": false
      }
    }
  ],
  "variables": {
    "BRONZE_BUCKET": "bronze",
    "SILVER_BUCKET": "silver",
    "GOLD_BUCKET": "gold",
    "S3_ENDPOINT": "http://minio:9000",
    "S3_REGION": "us-east-1",
    "WAREHOUSE_SCHEMA_STAGING": "staging",
    "WAREHOUSE_SCHEMA_GOLD": "gold"
  }
}
